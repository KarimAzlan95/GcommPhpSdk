{
    "name": "stexpert/gcomm-php-sdk",
    "description": "Package for interacting with Gcomm rest API.",
    "type": "library",
    "require": {
        "guzzlehttp/guzzle": "7.5.x-dev",
        "codexpertmy/tests": "dev-master",
        "php-http/guzzle7-adapter": "1.x-dev"
    },
    "require-dev": {
        "phpunit/phpunit": "9.6.x-dev"
    },
    "license": "private",
    "autoload": {
        "psr-4": {
            "Stexpert\\GcommPhpSdk\\": "src/"
        }
    },
    "authors": [
        {
            "name": "ST Expert"
        }
    ],
    "minimum-stability": "dev",
    "config": {
        "allow-plugins": {
            "php-http/discovery": false
        }
    }
}
